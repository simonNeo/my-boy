{
  "name": "server",
  "version": "0.0.1",
  "description": "server",
  "author": "Simon<simon@sayto.top>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "npm run start:dev",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:demo": "node dist/src/business/main",
    "start:prod": "node dist/src/business/main",
    "sequelize": "node -r ts-node/register ./node_modules/.bin/sequelize",
    "sequelize:prod": "NODE_ENV=production node -r ts-node/register ./node_modules/.bin/sequelize",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "migrate": "yarn sequelize db:migrate",
    "migrate:undo": "yarn sequelize db:migrate:undo",
    "migrate:new": "yarn sequelize migration:generate",
    "seed": "yarn sequelize db:seed:all",
    "seed:new": "yarn sequelize seed:generate",
    "sequelize-auto": "sequelize-auto"
  },
  "dependencies": {
    "@alicloud/dysmsapi20170525": "^2.0.23",
    "@alicloud/openapi-client": "^0.4.6",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^9.0.2",
    "@nestjs/swagger": "^6.3.0",
    "async": "^3.2.5",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "decimal.js": "^10.4.3",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "minio": "^8.0.0",
    "moment": "^2.29.4",
    "mysql2": "^3.2.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/async": "^3.2.24",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "sequelize-auto": "^0.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
